{
  "name": "react-antd",
  "version": "2.0.0",
  "author": "Dmitry Sheiko (http://dsheiko.com)",
  "main": "index.js",
  "scripts": {
    "start": "DEBUG=HTTP,ERROR,INFO node index.js",
    "build": "webpack --config webpack.backend.js && webpack --config webpack.frontend.js",
    "watch": "echo '*************' && echo 'Node.js server listening on http://127.0.0.1:9100/' && echo '*************' && npm-run-all --parallel watch:server build:server build:client",
    "watch:server": "nodemon --inspect ./index.js 9100",
    "build:client": "webpack --watch --config webpack.frontend.js",
    "build:server": "webpack --watch --config webpack.backend.js",
    "migrate:up": "db-migrate up",
    "migrate:down": "db-migrate down"
  },
  "keywords": [],
  "engines": {
    "node": ">=16"
  },
  "license": "ISC",
