  "main": "index.js",
  "scripts": {
    "start": "DEBUG=HTTP,ERROR,INFO node index.js",
    "build": "webpack --config webpack.backend.js && webpack --config webpack.frontend.js",
    "watch": "concurrently --kill-others \"webpack --watch  --config webpack.frontend.js\" \"webpack --watch --config webpack.backend.js\"",
    "migrate:up": "db-migrate up",
    "migrate:down": "db-migrate down",
    "lint": "eslint --ext=js,jsx ./frontend/src",
    "lint:fix": "eslint --ext=js,jsx --fix ./frontend/src",
    "backend:lint": "eslint --ext=js,jsx ./backend/src",
    "backend:lint:fix": "eslint --ext=js,jsx --fix ./backend/src"
  },
  "keywords": [],
  "engines": {
    "node": ">=11"
  },
