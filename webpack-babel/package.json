{
  "name": "boilerplate-webpack-babel",
  "version": "0.0.1",
  "author": "Dmitry Sheiko (http://dsheiko.com)",
  "keywords": [
    "webpack",
    "babel",
    "optimization",
    "starter",
    "boilerplate",
    "hot"
  ],
  "scripts": {
    "build:dev": "webpack --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js && webpack --config webpack.prod-legacy.js",
    "watch": "webpack --watch  --config webpack.dev.js",
    "start": "http-server . -o -c-1",
    "lint": "eslint --ext=js,jsx ./src",
    "lint:fix": "eslint --ext=js,jsx --fix ./src"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^0.1.17",
    "eslint": "^5.7.0",
    "eslint-plugin-import": "^2.14.0",
    "http-server": "^0.11.1",
    "terser-webpack-plugin": "^1.1.0",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.1"
  },
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "console-log-html": "^2.0.2"
  }
}
